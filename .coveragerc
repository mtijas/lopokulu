[run]
branch = True
source = ./src
omit =
    *migrations*
    *tests*
    */asgi.py
    */wsgi.py
    */manage.py
    */__init__.py

[report]
exclude_lines =
    pragma: no cover

    raise AssertionError
    raise NotImplementedError

    if 0:
    if __name__ == .__main__.:

    @(abc\.)?abstractmethod

ignore_errors = True

[html]
directory = coverage_html_report
